{
    "name": "backend",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "npm run prod",
        "build": "npm-run-all clean transpile",
        "server": "node ./dist-server/bin/www",
        "prod": "npm-run-all build server",
        "transpile": "babel ./src --out-dir dist-server --copy-files",
        "clean": "rimraf dist-server",
        "dev": "nodemon ./dist-server/bin/www",
        "dev:build": "babel --watch ./src --out-dir dist-server --copy-files"
    },
    "dependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/preset-env": "^7.16.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "express": "~4.16.1",
        "morgan": "~1.9.1",
        "npm-run-all": "^4.1.5",
        "rimraf": "^3.0.2",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.16.0",
        "@babel/eslint-plugin": "^7.14.5",
        "eslint": "^8.1.0",
        "nodemon": "^2.0.14"
    },
    "nodemonConfig": {
        "watch": [
            "dist-server/*"
        ],
        "ignore": [
            "**/__tests__/**",
            "*.test.js",
            "*.spec.js"
        ]
    }
}
